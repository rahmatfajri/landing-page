{
  "containerDefinitions": [
    {
      "name": "server",
      "image": "081602682094.dkr.ecr.us-east-2.amazonaws.com/sigma-bp-lp:BUILD_NUMBER",
      "cpu": 0,
      "memory": 256,
      "portMappings": [
        {
          "containerPort": 81
        }
      ],
      "dockerLabels" : {
        "container" : "server",
        "environment" : "development"
      },
      "logConfiguration" : {
        "logDriver": "json-file",
        "options": {
          "labels" : "container,environment"
        }
      }
    }]      
}
